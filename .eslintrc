{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 1,
    "no-undef": 2,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "semi": 1,
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-debugger": 1,
    "spaced-comment": 1,
    "react/require-default-props": 1,
    "indent": 1,
    "react/no-unused-prop-types": 1,
    "react/jsx-no-bind": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 1,
    "class-methods-use-this": 1,
    "no-param-reassign": [2, {
      "props": false
    }],
    "max-len": [1, { "code": 200 }],
    "padded-blocks": 1,
    "no-unreachable": 1,
    "prefer-const": 1,
    "react/prefer-stateless-function": 0
  }
}
